@article{broomell2019,
  title = {Interpreting {{Model Comparison Requires Understanding Model-Stimulus Relationships}}},
  author = {Broomell, Stephen B. and Sloman, Sabina J. and Blaha, Leslie M. and Chelen, Julia},
  year = {2019},
  month = dec,
  journal = {Computational Brain \& Behavior},
  volume = {2},
